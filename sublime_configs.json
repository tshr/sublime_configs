{
  "binary_file_patterns":
  [
    "node_modules/*",
    "public/*"
  ],
  "color_scheme": "Packages/Color Scheme - Default/Monokai Bright.tmTheme",
  "default_line_ending": "unix",
  "font_size": 12,
  "ignored_packages":
  [
    "Vintage"
  ],
  "rulers":
  [
    100
  ],
  "tab_size": 2,
  "translate_tabs_to_spaces": true,
  "trim_trailing_white_space_on_save": true,
  "word_wrap": false
}
